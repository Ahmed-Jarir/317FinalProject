                                 ; Asynchronous Normal Mode (U2X = 0)
                                 .equ BAUD_UBRR = (F_OSC / (16 * BAUD_RATE)) - 1
                                 
                                 ; Maximum Command Length
                                 .equ CMD_MAX_LEN = 100
                                 
                                 
